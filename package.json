{
  "name": "aurelia-router",
  "version": "1.6.3",
  "description": "A powerful client-side router.",
  "keywords": [
    "aurelia",
    "router"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia/router/issues"
  },
  "license": "MIT",
  "author": "Rob Eisenberg <rob@bluespire.com> (http://robeisenberg.com/)",
  "main": "dist/commonjs/aurelia-router.js",
  "module": "dist/es2015/aurelia-router.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/aurelia/router"
  },
  "scripts": {
    "start": "npm run dev -- --format es2015",
    "dev": "node build/scripts/dev",
    "build": "node build/scripts/build",
    "bundle-dts": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "karma": "karma start",
    "test": "npm run karma -- --unit --integration --single-run --browsers ChromeHeadless",
    "test:watch": "npm run karma -- --unit --integration --coverage --browsers ChromeHeadless",
    "test:debugger": "npm run karma -- --unit --integration --coverage --browsers ChromeDebugging",
    "test:unit": "npm run karma -- --unit",
    "test:integration": "npm run karma -- --integration",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts' --force"
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "aurelia-router",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "peerDependencies": {
      "aurelia-dependency-injection": "^1.0.0",
      "aurelia-event-aggregator": "^1.0.0",
      "aurelia-history": "^1.1.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-path": "^1.0.0",
      "aurelia-route-recognizer": "^1.0.0"
    },
    "dependencies": {
      "aurelia-dependency-injection": "^1.0.0",
      "aurelia-event-aggregator": "^1.0.0",
      "aurelia-history": "^1.1.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-path": "^1.0.0",
      "aurelia-route-recognizer": "^1.2.0"
    }
  },
  "dependencies": {
    "aurelia-dependency-injection": "^1.0.0",
    "aurelia-event-aggregator": "^1.0.0",
    "aurelia-history": "^1.1.0",
    "aurelia-logging": "^1.0.0",
    "aurelia-path": "^1.0.0",
    "aurelia-route-recognizer": "^1.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/webpack": "^4.4.17",
    "@types/webpack-env": "^1.13.6",
    "aurelia-binding": "^2.1.5",
    "aurelia-bootstrapper": "^2.3.0",
    "aurelia-framework": "^1.3.0",
    "aurelia-history-browser": "^1.2.0",
    "aurelia-loader-webpack": "^2.2.1",
    "aurelia-metadata": "^1.0.4",
    "aurelia-pal": "^1.8.0",
    "aurelia-pal-browser": "^1.8.0",
    "aurelia-templating": "^1.10.0",
    "aurelia-templating-binding": "^1.5.1",
    "aurelia-templating-resources": "^1.7.1",
    "aurelia-templating-router": "^1.3.3",
    "aurelia-testing": "^1.0.0",
    "aurelia-tools": "0.2.4",
    "aurelia-webpack-plugin": "^3.0.0",
    "dts-bundle-generator": "^1.6.1",
    "gulp-watch": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.4.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "rimraf": "^2.6.2",
